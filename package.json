{
  "name": "@thespielplatz/nuxt-dev-base",
  "homepage": "https://github.com/thespielplatz/nuxt-dev-base",
  "version": "1.3.0",
  "description": "Development base module for Nuxt",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thespielplatz/nuxt-dev-base.git"
  },
  "license": "MIT",
  "private": false,
  "type": "module",
  "workspaces": [
    "playground"
  ],
  "meta": {
    "special-version": "0.0.1-for-testing"
  },
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.mjs",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/types.d.mts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "npm run dev:prepare && nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
  },
  "dependencies": {
    "@nuxt/kit": "^4.2.2",
    "@nuxt/ui": "^4.3.0",
    "consola": "^3.4.2",
    "defu": "^6.1.4",
    "zod": "^3.3.5"
  },
  "devDependencies": {
    "@nuxt/devtools": "^3.1.1",
    "@nuxt/eslint-config": "^1.12.1",
    "@nuxt/module-builder": "^1.0.2",
    "@nuxt/schema": "^4.2.2",
    "@nuxt/test-utils": "^3.23.0",
    "@types/node": "latest",
    "changelogen": "^0.6.2",
    "eslint": "^9.39.2",
    "nuxt": "^4.2.2",
    "typescript": "~5.9.3",
    "vitest": "^3.2.4",
    "vue-tsc": "3.2.2"
  }
}
